{
  "messaging_product": "whatsapp",
  "to": "{{to}}",
  "type": "template",
  "template": {
    "name": "restaurant_reservation",
    "language": {
      "code": "{{language_code}}"
    },
    "components": [
      {
        "type": "body",
        "parameters": [
          {
            "type": "text",
            "text": "{{user_name}}"
          },
          {
            "type": "text",
            "text": "{{phone_number}}"
          },
          {
            "type": "text",
            "text": "{{guests}}"
          },
          {
            "type": "date_time",
            "date_time": {
              "fallback_value": "{{reservation_date}}"
            }
          },
          {
            "type": "date_time",
            "date_time": {
              "fallback_value": "{{reservation_time}}"
            }
          },
          {
            "type": "text",
            "text": "{{message}}"
          }
        ]
      }
    ]
  }
}